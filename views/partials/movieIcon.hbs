
<a class="movieIcon" href="/list/{{_id}}">
<figure class="progressive">
  <img class="progressive__img progressive--not-loaded" data-progressive={{img_s}} src="">
</figure>

       
        <div class="h-5 movieIcon__name_container"></div>
        <div class="movieIcon__overflow">
        <div class="movieIcon_name">
            {{name}}
            </div>
            <div class="movieIcon_runtime">
                {{!-- {{convertToMin runtime}} Mins --}}
                {{genre}}
            </div>
        </div>

    {{#if featured}}
        <div class="featured_tag">
        </div>
       {{/if}} 
</a>
     