    <div class="second_carousel t-w100" style="  height: 60vh;
  width: 50%;">
        <div class="single_carousel">
            {{#each imagesForCarousel}}
                <img src={{this}} alt="">
            {{/each}}
        </div>
    </div>

   
</div>


